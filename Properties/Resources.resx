    <value>INSERT OR REPLACE INTO VulnerabilitesCCIs VALUES (@Vulnerability_ID, (SELECT CCI_ID FROM CCIs WHERE CCI = @CCI));</value>
  </data>
  <data name="MapVulnerabilityToIAControl" xml:space="preserve">
